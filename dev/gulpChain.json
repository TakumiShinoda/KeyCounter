{
  "copyChain": [
    {
      "src": "src/assets/javascript/index/webviewResources/**",
      "dase": "src/assets/javascript/index/webviewResources/",
      "dest": "./dist/js/index/webviewResources"
    },
    {
      "src": "src/assets/javascript/node_dependencies.js",
      "base": "src/assets/javascript",
      "dest": "./dist/js"
    },
    {
      "src": "src/assets/javascript/third_party/bootstrap/*",
      "base": "src/assets/javascript/third_party/",
      "dest": "./dist/js"
    },
    {
      "src": "src/assets/javascript/third_party/ECharts/*",
      "base": "src/assets/javascript/third_party/",
      "dest": "./dist/js"
    },
    {
      "src": "src/assets/css/third_party/bootstrap/*",
      "base": "src/assets/css/third_party",
      "dest": "./dist/css"
    }
  ],
  "routes": [
    "index",
    "checkPermission"
  ]
}
